         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = cl::sycl::clamp(acc_" << OpName << "_hidden_state[id], " << -fAttrClip << ", " << fAttrClip << ");\n";
      }  

      if (fAttrActivations[direction] == "Relu") {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = cl::sycl::max(acc_" << OpName << "_hidden_state[id], 0.0f);\n";
      }
      else if (fAttrActivations[direction] == "Tanh") {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = cl::sycl::tanh(acc_" << OpName << "_hidden_state[id]);\n";
      }
      else if (fAttrActivations[direction] == "Sigmoid") {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = cl::sycl::native::recip(1 + cl::sycl::exp(- acc_" << OpName << "_hidden_state[id]));\n";
      }
      else if (fAttrActivations[direction] == "Affine") {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = " << fAttrActivationAlpha[direction];
         out << " * acc_" << OpName << "_hidden_state[i] + " << fAttrActivationBeta[direction] << ";\n";
      }
      else if (fAttrActivations[direction] == "ScaledTanh") {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = " << fAttrActivationAlpha[direction] << " * cl::sycl::tanh(" << fAttrActivationBeta[direction] << " * acc_" << OpName << "_hidden_state[id]);\n"; 
      }

      out << SP*5 << "});\n";
      out << SP*4 << "});\n";
      
      out << SP*3 << "}\n";
   }

      // Padding the hidden state for RNN with different sequency lengths
   if (!fNSequence_lens.empty()) {
      out << SP*3 << "q.submit([&](cl::sycl::handler& cgh){\n";
      out << SP*4 << "auto acc_tensor_" << fNSequence_lens << " = cl::sycl::accessor{buf_tensor_" << fNSequence_lens;
      out << ", cgh, cl::sycl::read_only};\n";
      out << SP*4 << "auto acc_" << OpName << "_hidden_state = cl::sycl::accessor{buf_" << OpName << "_hidden_state, cgh, cl::sycl::write_only};\n";
         
      out << SP*4 << "cgh.parallel_for<class " << OpName << "_pad>(cl::sycl::range<3>(" << seq_length << ", " << batch_size << ", " << fAttrHiddenSize;
      out << "), [=](cl::sycl::id<3> id){\n";
      out << SP*5 << "auto seq = id[0];\n";
      out << SP*5 << "auto batch = id[1];\n";
      out << SP*5 << "auto h = id[2];\n";

      out << SP*5 << "if (seq >= acc_tensor_" << fNSequence_lens << "[batch]) {\n";
      if (num_directions == 1) {
         out << SP*6 << "acc_" << OpName << "_hidden_state[seq * " << num_directions * batch_size * fAttrHiddenSize << " + batch * " << fAttrHiddenSize << " + h ] = 0.0;\n";
      }
      else {
         out << SP*6 << "acc_" << OpName << "_hidden_state[seq * " << num_directions * batch_size * fAttrHiddenSize << " + batch * " << fAttrHiddenSize << " + h ] = 0.0;\n";
         out << SP*6 << "acc_" << OpName << "_hidden_state[seq * " << num_directions * batch_size * fAttrHiddenSize << " + " << batch_size * fAttrHiddenSize << " + batch * " << fAttrHiddenSize << " + h] = 0.0;\n";
      }

      out << SP*4 << "});\n";
      out << SP*3 << "});\n";
   }

   // Copy the hidden state into y and y_h
   if (fAttrLayout == 0) {
      if (!fNY_h.empty()) {
         if (fNSequence_lens.empty()) {
            size_t yh_size = batch_size * fAttrHiddenSize;

            if (fAttrDirection == "backward") {
               out << SP*3 << "q.submit([&](cl::sycl::handler& cgh){\n";
               out << SP*4 << "auto acc_" << OpName << "_hidden_state = cl::sycl::accessor{buf_" << OpName << "_hidden_state, cgh, cl::sycl::read_only};\n";
               out << SP*4 << "auto acc_tensor_" << fNY_h << "= cl::sycl::accessor{buf_tensor_" << fNY_h << ", cgh, cl::sycl::write_only, cl::sycl::no_init};\n";

               out << SP*4 << "cgh.parallel_for<class " << OpName << "_copy_y_to_y_h>(cl::sycl::range<1>(" << yh_size << "), [=](cl::sycl::id<1> id){\n";
               out << SP*5 << "acc_tensor_" << fNY_h << "[id] = acc_" << OpName << "_hidden_state[id];\n";
               out << SP*4 << "});\n";
               out << SP*3 << "});\n";
            }
            else {
               size_t offset = (seq_length - 1) * num_directions * batch_size * fAttrHiddenSize;
               out << SP*3 << "q.submit([&](cl::sycl::handler& cgh){\n";
               out << SP*4 << "auto acc_" << OpName << "_hidden_state = cl::sycl::accessor{buf_" << OpName << "_hidden_state, cgh, cl::sycl::read_only};\n";
               out << SP*4 << "auto acc_tensor_" << fNY_h << "= cl::sycl::accessor{buf_tensor_" << fNY_h << ", cgh, cl::sycl::write_only, cl::sycl::no_init};\n";

               out << SP*4 << "cgh.parallel_for<class " << OpName << "_copy_y_to_y_h>(cl::sycl::range<1>(" << yh_size << "), [=](cl::sycl::id<1> id){\n";
               out << SP*5 << "acc_tensor_" << fNY_h << "[id] = acc_" << OpName << "_hidden_state[id + " << offset << "];\n";
               out << SP*4 << "});\n";
               out << SP*3 << "});\n";
            }

            if (num_directions == 2) {
               out << SP*3 << "q.submit([&](cl::sycl::handler& cgh){\n";
               out << SP*4 << "auto acc_" << OpName << "_hidden_state = cl::sycl::accessor{buf_" << OpName << "_hidden_state, cgh, cl::sycl::read_only};\n";
               out << SP*4 << "auto acc_tensor_" << fNY_h << " = cl::sycl::accessor{buf_tensor_" << fNY_h;
               out << ", cgh, cl::sycl::write_only};\n";
               out << SP*4 << "cgh.parallel_for<class " << OpName << "_copy_y_to_y_h_bidirectional>(cl::sycl::range<1>(" << yh_size;
               out << "), [=](cl::sycl::id<1> id){\n";
               out << SP*5 << "acc_tensor_" << fNY_h << "[id + " << yh_size << "] = acc_" << OpName << "_hidden_state[id + " << yh_size << "];\n"
               out << SP*4 << "});\n";
               out << SP*3 << "});\n";
            }
         }
         else {
            if (fAttrDirection == "backward")
            out << SP*3 << "q.submit([&](cl::sycl::handler& cgh){\n";
            out << SP*4 << "auto acc_" << OpName << "_hidden_state = cl::sycl::accessor{buf_" << OpName << "_hidden_state, cgh, cl::sycl::read_only};\n";
            out << SP*4 << "auto acc_tensor_" << fNY_h << "= cl::sycl::accessor{buf_tensor_" << fNY_h << ", cgh, cl::sycl::write_only, cl::sycl::no_init};\n";

            out << SP*4 << "cgh.parallel_for<class " << OpName << "_copy_y_to_y_h>(cl::sycl::range<1>(" << batch_size * fAttrHiddenSize << "), [=](cl::sycl::id<1> id){\n";
            out << SP*5 << "acc_tensor_" << fNY_h << "[id] = acc_" << OpName << "_hidden_state[id];\n";
            out << SP*4 << "});\n";
            out << SP*3 << "});\n";
         }
      }
   }
   return out.str();
}

template<typename T>
auto ROperator_RNN<T>::GenerateGPU(std::string OpName)
-> std::string {

   OpName = "op_" + OpName;
	std::stringstream out;

	size_t seq_length = (fAttrLayout == 0) ? fShapeX[0] : fShapeX[1];
	size_t batch_size = (fAttrLayout == 0) ? fShapeX[1] : fShapeX[0];
	size_t input_size = fShapeX[2];
   size_t num_directions = fShapeW[0];
   
   // set the input
   out << "\n";
   out << SP*3 << "auto tensor_" << fNX << " = fTensor_" << fNX << ".data();\n";
   
   if (fAttrLayout == 0) {
      if (fType == "float") {
         out << SP*3 << "float *" << OpName << "_input = tensor_" << fNX << ";\n";
      }
   } else {
      if (fUseSession)
         out << SP*3 << fType << " * " << OpName << "_input = fVec_" << OpName << "_input.data();\n";
      else
         out << SP*3 << fType << " " << OpName << "_input[" << seq_length * batch_size * input_size << "];\n";
      
      out << SP*3 << "for(size_t seq = 0; seq < " << seq_length << "; seq++) {\n";
      out << SP*4 << "for(size_t batch = 0; batch < " << batch_size << "; batch++) {\n";
      out << SP*5 << "for(size_t i = 0; i < " << input_size << "; i++) {\n";
      out << SP*6 << OpName << "_input[seq * " << batch_size * input_size 
          << " + batch * " << input_size << " + i] = " << "tensor_" << fNX << "[batch * "
          << seq_length * input_size << " + seq * " << input_size << " + i];\n";
      out << SP*5 << "}\n";
      out << SP*4 << "}\n";
      out << SP*3 << "}\n";
   }

   out << "\n";
   // Set the initial hidden state
   if (!fNInitial_h.empty()) {
      if (fAttrLayout == 0) {
         out << SP*3 << fType << " *" << OpName << "_initial_hidden_state = " << " tensor_"
                << fNInitial_h << ";\n";
         out << SP*3 << "auto buf_" << OpName << "_initial_hidden_state = cl::sycl::buffer{fTensor_" << fNInitial_h << ".data(), cl::sycl::range<1>(fTensor_" << fNInitial_h << ".size())};\n";
      } else {
         if (fUseSession) {
            out << SP*3 << fType << " * " << OpName << "_initial_hidden_state = fVec_" << OpName
                << "_initial_hidden_state.data();\n";
            out << SP*3 << "auto buf_" << OpName << "_initial_hidden_state = cl::sycl::buffer{fVec_" << OpName << "_initial_hidden_state.data(), cl::sycl::range<1>(fVec_" << OpName << "_initial_hidden_state.size())};\n";
         }
         else {
            out << SP*3 << fType << " " << OpName << "_initial_hidden_state[" << num_directions * batch_size *
               fAttrHiddenSize << "] = {0};\n";
            out << "std::vector<" << fType << ", " << num_directions * batch_size * fAttrHiddenSize << "> vec_" << OpName << "_initial_hidden_state = {0};\n";
            out << SP*3 << "auto buf_" << OpName << "_initial_hidden_state = cl::sycl::buffer{vec_" << OpName << "_initial_hidden_state.data(), cl::sycl::range<1>(vec_" << OpName << "_initial_hidden_state.size())};\n";
         }
         for (size_t direction = 0; direction < num_directions; direction++) {
            out << SP*3 << "for(size_t batch = 0; batch < " << batch_size << "; batch++) {\n";
            out << SP*4 << "for(size_t h = 0; h < " << fAttrHiddenSize << "; h++) {\n";
            out << SP*5 << OpName << "_initial_hidden_state["
                << direction * batch_size * fAttrHiddenSize << " + batch * " << fAttrHiddenSize 
                << " + h] = tensor_" << fNInitial_h << "[batch * " << num_directions * fAttrHiddenSize
                << " + " << direction * fAttrHiddenSize << " + h];\n";
            out << SP*4 << "}\n";
            out << SP*3 << "}\n";
         }
      }
   }

   out << "\n";
   if (fUseSession)
      out << SP << fType << " * " << OpName << "_feedforward = fVec_" << OpName
          << "_feedforward.data();\n";
   else 
      out << SP << fType << " " << OpName << "_feedforward[" << seq_length * batch_size * fAttrHiddenSize << "] = {0};\n";

   // Set the hidden state
   out << "\n";
   if (fAttrLayout == 0 && !fNY.empty()) {
      out << SP*3 << fType << " * " << OpName << "_hidden_state = tensor_" << fNY << ";\n";
      out << SP*3 << "auto buf_" << OpName << "_hidden_state = buf_tensor_" << fNY << ";\n";
   }
   else {
      if (fUseSession) {
         out << SP*3 << fType << " * " << OpName << "_hidden_state = fVec_" << OpName << "_hidden_state.data();\n";
         out << SP*3 << "auto buf_" << OpName << "_hidden_state = cl::sycl::buffer{fVec_" << OpName << "_hidden_state.data(), cl::sycl::range<1>(fVec_" << OpName << "_hidden_state.size())};\n";
      }
      else {
         out << SP*3 << fType << " * " << OpName << "_hidden_state = [" << seq_length * num_directions * batch_size * fAttrHiddenSize << "]\n";
         out << SP*3 << "std::vector<" << fType << ", " << seq_length * num_directions * batch_size * fAttrHiddenSize << "> " << OpName << "_hidden_state = {0};\n";
         out << SP*3 << "auto buf_" << OpName << "_hidden_state = cl::sycl::buffer{" << OpName << "_hidden_state.data(), cl::sycl::range<1>(" << OpName << "_hidden_state.size())};\n";
      }
   }

   out << "\n";
   out << SP*3 << "oneapi::mkl::transpose " << OpName <<  "_transA = oneapi::mkl::transpose::nontrans;\n";
   out << SP*3 << "oneapi::mkl::transpose " << OpName << "_transB = oneapi::mkl::transpose::trans;\n";
   out << SP*3 << "int " << OpName << "_m = " << seq_length * batch_size << ";\n";
   out << SP*3 << "int " << OpName << "_n = " << fAttrHiddenSize << ";\n";
   out << SP*3 << "int " << OpName << "_k = " << input_size << ";\n";
   if (fType == "float") {
      out << SP*3 << "float " << OpName << "_alpha = 1.;\n";
      out << SP*3 << "float " << OpName << "_beta = .0;\n";
   }
   if (!fNB.empty()) {
      out << SP*3 << "int " << OpName << "_bias_size = " << seq_length * batch_size * fAttrHiddenSize << ";\n";
      out << SP*3 << "int " << OpName << "_incx = 1;\n";
      out << SP*3 << "int " << OpName << "_incy = 1;\n";
   }

   for (size_t direction = 0; direction < num_directions; direction++) {
      out << "\n";
      if (fType == "float") {
         if (direction == 0) {
            out << SP*3 << "auto buf_" << OpName << "_feedforward = cl::sycl::buffer{fVec_" << OpName << "_feedforward.data(), cl::sycl::range<1>(fVec_" << OpName << "_feedforward.size())};\n";
            if (fAttrLayout == 0)
               out << SP*3 << "auto buf_" << OpName << "_input = cl::sycl::buffer{fTensor_" << fNX << ".data(), cl::sycl::range<1>(fTensor_" << fNX << ".size())};\n";
            else 
               out << SP*3 << "auto buf_" << OpName << "_input = cl::sycl::buffer{fVec_" << OpName << "_input" << ".data(), cl::sycl::range<1>(fVec_" << OpName << "_input" << ".size())};\n";
         
            out << SP*3 << "oneapi::mkl::blas::gemm(q, " << OpName << "_transB, " << OpName << "_transA, ";
            out << OpName << "_n, " << OpName << "_m, " << OpName << "_k, " << OpName << "_alpha, ";
            out << "buf_tensor_" << fNW << ", " << OpName << "_k, buf_" << OpName << "_input, " << OpName << "_k, ";
            out << OpName << "_beta, buf_" << OpName << "_feedforward, " << OpName << "_n);\n"; 
         }
         else {
            out << SP*3 << "size_t " << OpName << "_w_offset = " << fAttrHiddenSize * input_size << ";\n";
            out << SP*3 << "auto buf_" << OpName << "_feedforward_" << direction << " = cl::sycl::buffer{fVec_" << OpName << "_feedforward.data(), cl::sycl::range<1>(fVec_" << OpName << "_feedforward.size())};\n";
            out << SP*3 << "auto buf_tensor_" << fNW << "_w_offset = cl::sycl::buffer{tensor_" << fNW << " + " << fAttrHiddenSize * input_size  << ", cl::sycl::range<1>(" << fAttrHiddenSize * input_size << ")};\n";
            if (fAttrLayout == 0)
               out << SP*3 << "auto buf_" << OpName << "_input_" << direction << " = cl::sycl::buffer{fTensor_" << fNX << ".data(), cl::sycl::range<1>(fTensor_" << fNX << ".size())};\n";
            else 
               out << SP*3 << "auto buf_" << OpName << "_input_" << direction << " = cl::sycl::buffer{fVec_" << OpName << "_input" << ".data(), cl::sycl::range<1>(fVec_" << OpName << "_input" << ".size())};\n";
         
            out << SP*3 << "oneapi::mkl::blas::gemm(q, " << OpName << "_transB, " << OpName << "_transA, ";
            out << OpName << "_n, " << OpName << "_m, " << OpName << "_k, " << OpName << "_alpha, ";
            out << "buf_tensor_" << fNW << "_w_offset, " << OpName << "_k, buf_" << OpName << "_input_" << direction << ", " << OpName << "_k, ";
            out << OpName << "_beta, buf_" << OpName << "_feedforward_" << direction << ", " << OpName << "_n);\n"; 
         }
       }

       out << "q.wait();\n";
       // add the bias

       if (!fNB.empty()) {
         out << "\n";
         if (fType == "float") {
            if (direction == 0) {
               out << SP*3 << "oneapi::mkl::blas::axpy(q, " << OpName << "_bias_size, " << OpName << "_alpha, ";
               out << "buf_tensor_" << fNB << ", " << OpName << "_incx, buf_" << OpName << "_feedforward, " << OpName << "_incy);\n";
            }
            else {
               out << SP*3 << "size_t " << OpName << "_bias_offset = " << seq_length * batch_size * fAttrHiddenSize << ";\n"; 
               out << SP*3 << "auto buf_tensor_" << fNB << "_bias_offset = cl::sycl::buffer{buf_tensor_" << fNB << ", cl::sycl::id<1>(" << OpName << "_bias_offset), cl::sycl::range<1>(" << OpName << "_bias_offset)};\n";
            
               out << SP*3 << "oneapi::mkl::blas::axpy(q, " << OpName << "_bias_size, " << OpName << "_alpha, ";
               out << "buf_tensor_" << fNB << "_bias_offset, " << OpName << "_incx, buf_" << OpName << "_feedforward, " << OpName << "_incy);\n";
            }
         }
       }

       out << "q.wait();\n";

      // copy feedforward into hidden state
      out << "\n";
      if (direction == 0) {
         out << SP*3 << "int size = " << batch_size * fAttrHiddenSize << ";\n"; // n
         out << SP*3 << "int incx = 1;\n";
         out << SP*3 << "int offset = " << batch_size * fAttrHiddenSize << ";\n";
         out << SP*3 << "int incy = 1;\n";
         out << SP*3 << "int h_offset = " << (num_directions + direction) * batch_size * fAttrHiddenSize << ";\n";
      }
      else {
         out << SP*3 << "h_offset += " << direction * batch_size * fAttrHiddenSize << ";\n";
      }
      out << SP*3 << "oneapi::mkl::blas::copy_batch(q, size, buf_" << OpName << "_feedforward, ";
      out << "incx, offset, buf_" << OpName << "_hidden_state, incy, h_offset, " << seq_length << ");\n"; 

      out << SP*3 << "q.wait();\n";
      
      out << "\n";
      out << SP*3 << "for (size_t seq = 0; seq < " << seq_length << "; seq++) {\n";
      if (fAttrDirection == "backward" || direction == 1) {
         out << SP*4 << "size_t index = " << seq_length - 1 << " - seq;\n";
      }
      else {
         out << SP*4 << "size_t index = seq;\n";
      }

      out << SP*4 << "int m2 = " << batch_size << ";\n";
      out << SP*4 << "size_t offset = index * " << num_directions * batch_size * fAttrHiddenSize << " + ";
      out << direction * batch_size * fAttrHiddenSize << ";\n";
      out << SP*4 << "size_t size = " << batch_size * fAttrHiddenSize << ";\n";
      out << SP*4 << "if (seq == 0) {\n";
      if (!fNInitial_h.empty()) {
         out << SP*5 << "size_t r_offset = " << direction * fAttrHiddenSize * fAttrHiddenSize << ";\n";
         out << SP*5 << "size_t initial_hidden_state_offset = " << direction * batch_size * fAttrHiddenSize << ";\n";

         out << SP*5 << "auto buf_tensor_" << fNR << "_offset = cl::sycl::buffer{buf_tensor_" << fNR << ", cl::sycl::id<1>(r_offset), cl::sycl::range<1>(" << fAttrHiddenSize * fAttrHiddenSize << ")};\n";
         out << SP*5 << "auto buf_" << OpName << "_initial_hidden_state_offset = cl::sycl::buffer{buf_" << OpName << "_initial_hidden_state, cl::sycl::id<1>(initial_hidden_state_offset), cl::sycl::range<1>(" << fAttrHiddenSize * batch_size << ")};\n";
         out << SP*5 << "auto buf_" << OpName << "_hidden_state_offset = cl::sycl::buffer{buf_" << OpName << "_hidden_state, cl::sycl::id<1>(offset), cl::sycl::range<1>(" << (num_directions + direction) * batch_size * fAttrHiddenSize << ")};\n";
         out << "\n";
         if (fType == "float") {
            out << SP*5 << "oneapi::mkl::blas::gemm(q, " << OpName << "_transB, " << OpName << "_transA, " << OpName << "_n, ";
            out << "m2, " << OpName << "_n, " << OpName << "_alpha, " << "buf_tensor_" << fNR << "_offset, " << OpName << "_n, ";
            out << "buf_" << OpName << "_initial_hidden_state_offset, " << OpName << "_n, " << OpName << "_alpha, ";
            out << "buf_" << OpName << "_hidden_state_offset, " << OpName << "_n);\n";
         }
         out << "q.wait();\n";
      } 
      out << SP*4 << "} else {\n";
      out << SP*5 << "size_t r_offset = " << direction * fAttrHiddenSize * fAttrHiddenSize << ";\n";
      if (fAttrDirection == "backward" || direction == 1) {
         out << SP*5 << "size_t previous_offset = (index + 1) * " << num_directions * batch_size * fAttrHiddenSize << " + " << direction * batch_size * fAttrHiddenSize << ";\n";
      }
      else {
         out << SP*5 << "size_t previous_offset = (seq - 1) * " << num_directions * batch_size * fAttrHiddenSize << " + " << direction * batch_size * fAttrHiddenSize << ";\n";
      }
      
      if (fType == "float") {
         out << SP*5 << "auto buf_tensor_" << fNR << "_offset = cl::sycl::buffer{buf_tensor_" << fNR << ", cl::sycl::id<1>(r_offset), cl::sycl::range<1>(" << fAttrHiddenSize * fAttrHiddenSize << ")};\n";
         out << SP*5 << "auto buf_" << OpName << "_hidden_state_previous_offset = cl::sycl::buffer{buf_" << OpName << "_hidden_state, cl::sycl::id<1>(previous_offset), cl::sycl::range<1>(" << fAttrHiddenSize * batch_size << ")};\n";
         out << SP*5 << "auto buf_" << OpName << "_hidden_state_offset = cl::sycl::buffer{buf_" << OpName << "_hidden_state, cl::sycl::id<1>(offset), cl::sycl::range<1>(" << (num_directions + direction) * batch_size * fAttrHiddenSize << ")};\n";
      
         out << SP*5 << "oneapi::mkl::blas::gemm(q, " << OpName << "_transB, " << OpName << "_transA, ";
         out << OpName << "_n, " << "m2, " << OpName << "_n, " << OpName << "_alpha, " << "buf_tensor_" << fNR << ", ";
         out << OpName << "_n, " << "buf_" << OpName << "_hidden_state_previous_offset, " << OpName << "_n, " << OpName << "_alpha, ";
         out << "buf_" << OpName << "_hidden_state_offset, " << OpName << "_n);\n"; 
      }

      out << "q.wait();\n";
      out << SP*4 << "}\n";
      out << "\n";
      out << SP*4 << "auto buf_" << OpName << "_hidden_state_offset = cl::sycl::buffer{buf_" << OpName << "_hidden_state, cl::sycl::id<1>(offset), cl::sycl::range<1>(" << fAttrHiddenSize * batch_size << ")};\n";
      out << SP*4 << "q.submit([&](cl::sycl::handler& cgh) {\n";
      out << SP*5 << "auto acc_" << OpName << "_hidden_state = cl::sycl::accessor{buf_" << OpName << "_hidden_state_offset, cgh, cl::sycl::read_write};\n";

      out << SP*5 << "cgh.parallel_for<class " << OpName << "_activation_" << direction << ">(cl::sycl::range<1>(" << batch_size * fAttrHiddenSize << "), [=](cl::sycl::id<1> id){\n";
      if (fAttrClip > .0) {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = cl::sycl::clamp(acc_" << OpName << "_hidden_state[id], " << -fAttrClip << ", " << fAttrClip << ");\n";
      }  

      if (fAttrActivations[direction] == "Relu") {
         out << SP*6 << "acc_" << OpName << "_hidden_state[id] = cl::sycl::max(acc_" << OpName << "_hidden_state[id], 0.0f);\n";
      }
      else if (fAttrActivations[direction] == "Tanh") {
